{
  "name": "rental-zkp-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "snarkjs": "^0.6.11",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "vitest run ../test/client/**/*.test.js",
    "test:watch": "vitest watch ../test/client/**/*.test.js",
    "test:coverage": "vitest run --coverage ../test/client/**/*.test.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx}",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/coverage-v8": "^1.3.1",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest-environment-jsdom": "^29.5.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "prettier": "^3.1.1"
  },
  "resolutions": {
    "process": "^0.11.10"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/__tests__/**"
    ],
    "testMatch": [
      "<rootDir>/../test/client/**/*.test.{js,jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "main": "craco.config.js",
  "directories": {
    "test": "../test/client"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Frontend client for Rental ZKP system",
  "vitest": {
    "environment": "jsdom",
    "globals": true,
    "setupFiles": ["./src/setupTests.js"],
    "testTimeout": 10000,
    "hookTimeout": 10000,
    "include": ["../test/client/**/*.test.js"],
    "exclude": ["node_modules", "dist", ".idea", ".git", "cache"],
    "environmentOptions": {
      "jsdom": {
        "resources": "usable"
      }
    }
  }
}
